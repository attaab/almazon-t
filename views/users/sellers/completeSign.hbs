<div class="container container-fluid container-signup">
    <form class="signup" method="POST" action="/seller/complete"  enctype="multipart/form-data">
      {{# if hasErrors}}
    <div class="alert alert-danger" style="width:80%;margin:auto;">
      {{# each errors}}
        {{this}}
      {{/ each}}
      <a href="#" class="close" data-dismiss="alert" aria-label="close"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></a>
    </div>
  {{/ if}}
  {{# if hasSuccess}}
    <div class="alert alert-success">
      {{# each success}}
        {{this}}
      {{/ each}}
      <a href="#" class="close" data-dismiss="alert" aria-label="close"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
      </a>
    </div>
  {{/ if}}
  {{# if loginErrors}}
    {{#each loginErrors}}
      <div class="alert alert-danger" style="width:80%;margin:auto;">
        <a href="#" class="close" data-dismiss="alert" aria-label="close"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
        </a>
        {{msg}}
      </div>
    {{/ each}}
  {{/ if}}
      {{#if errors}}
        {{#each errors}}
          <div class="alert alert-danger">
            <a href="#" class="close" data-dismiss="alert" aria-label="close">
              <span class="glyphicon glyphicon-remove"></span>
            </a>
            {{msg}}
          </div>
        {{/each}}
      {{/if}}
      <div class="form-group">
        <p class="danger" id="alert-company" style="display:none;"></p>
        <input type="text" name="companyname" class="companyname sd" id="companyname" placeholder="اسم الشركة">
      </div>
      <div class="form-group">
      <p class="danger" id="alert-sellerId" style="display:none;"></p>
        <input type="text" name="sellerId" class="sellerId sd" id="sellerId" placeholder="معرف البطاقة الشخصية">
      </div>
      <div class="form-group">
        <input type="file" name="sellerimg" class="sellerimg" id="sellerimg" style="display:none;" accept="image/*">
        <button type="button" class="uploadid"><i class="fa fa-upload" aria-hidden="true"></i>  صورة الهوية الشخصية</button>
      </div>
      <input type="submit" class="btn btn-primary submit" value="submit" id="submi" style="">
  </form>
